{
  "name": "eighteen73/wordpress-beacon",
  "description": "Send website information to the developer's monitoring service for integrity checks.",
  "type": "wordpress-plugin",
  "license": "MIT",
  "authors": [
    {
      "name": "Ed Jeavons",
      "email": "ed@eighteen73.co.uk"
    }
  ],
  "require-dev": {
    "php": ">=7.4",
    "eighteen73/wordpress-coding-standards": "^2.0.0"
  },
  "minimum-stability": "stable",
  "autoload": {
    "psr-4": {
      "Eighteen73\\ConditionReport\\": "includes/classes/"
    }
  },
  "config": {
    "allow-plugins": {
      "dealerdirect/phpcodesniffer-composer-installer": true
    }
  }
}
