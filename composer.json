{
    "name": "eighteen73/website-monitor",
    "description": "Send website information to the developer's monitoring service for integrity checks.",
    "type": "wordpress-plugin",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Eighteen73\\WebsiteMonitor\\": "includes/classes/"
        }
    },
    "authors": [
        {
            "name": "Ed Jeavons",
            "email": "ed@orphans.co.uk"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "php": ">=7.4"
    },
    "require-dev": {
        "eighteen73/phpcs-composer": "^v1.0"
    },
    "config": {
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": true
        }
    },
    "scripts": {
        "test": [
            "phpcs"
        ]
    }
}
